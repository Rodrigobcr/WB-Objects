    me-&gt;search_views( ).
    me-&gt;search_domains( ).
    me-&gt;search_data_elements( ).
    me-&gt;search_structures( ).
    me-&gt;search_table_types( ).
    me-&gt;search_lock_objects( ).
    me-&gt;search_search_helps( ).


endmethod.</source>
